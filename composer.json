{
    "name": "anime-db/smotret-anime-browser-bundle",
    "license": "GPL-3.0",
    "type": "library",
    "description": "Smotret-Anime.ru API browser",
    "homepage": "http://github.com/anime-db/smotret-anime-browser-bundle",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\SmotretAnimeBrowserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnimeDb\\Bundle\\SmotretAnimeBrowserBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/guzzle": "~6.2"
    },
    "require-dev": {
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/expression-language": "~2.7|~3.0",
        "symfony/config": "~2.7|~3.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "phpunit/phpunit": "~4.8",
        "scrutinizer/ocular": "~1.3",
        "satooshi/php-coveralls": "^1.0"
    }
}
